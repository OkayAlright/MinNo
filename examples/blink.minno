   An example blink program.
   
/#

let outputPin : int = 13; //set pin for reference

def setup none -> none {
    //set mode to output
    pinMode outputPin OUTPUT;
}

def loop none -> none {
    // blink on and off with 1 sec intervals
    digitalWrite outputPin HIGH;
    delay 1000;
    digitalWrite outputPin LOW;
}
